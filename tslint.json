{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [
      true,
      "log"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "semicolon": [
      true,
      "never"
    ],
    "object-literal-sort-keys": false
  }
}